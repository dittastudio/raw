    "@tailwindcss/vite": "^4.1.18",
    "@unhead/vue": "^2.1.2",
    "@vueuse/core": "^14.2.0",
    "gsap": "^3.14.2",
    "keen-slider": "^6.8.6",
    "lenis": "^1.3.17",
    "linkedom": "^0.18.12",
    "nuxt": "^4.3.0",
