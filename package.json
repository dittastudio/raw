    "@nuxt/image": "^2.0.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^25.2.0",
    "del-cli": "^7.0.0",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "storyblok": "^4.14.1",
