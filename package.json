{
  "name": "raw",
  "type": "module",
  "private": true,
  "author": "ditta",
  "scripts": {
    "sb:login": "storyblok login",
    "sb:logout": "storyblok logout",
    "sb:help": "storyblok --help",
    "sb:components": "storyblok components pull --space 289672313529140 --verbose",
    "sb:types": "storyblok types generate --space 289672313529140 --strict",
    "sb:generate": "npm run sb:components && npm run sb:types",
    "dev": "npm run sb:generate && nuxt dev --host 0.0.0.0",
    "dev:ssl": "local-ssl-proxy --source 3010 --target 3000 --cert localhost.pem --key localhost-key.pem",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "nuke": "del-cli '.output' 'dist' '.nuxt' 'node_modules' 'package-lock.json'"
  },
  "dependencies": {
    "@storyblok/nuxt": "^8.2.3",
    "@tailwindcss/vite": "^4.1.18",
    "@unhead/vue": "^2.1.1",
    "@vueuse/core": "^14.1.0",
    "gsap": "^3.14.2",
    "lenis": "^1.3.17",
    "nuxt": "^4.2.2",
    "postcss-nested": "^7.0.2",
    "tailwindcss": "^4.1.18",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@nuxt/eslint": "^1.12.1",
    "@nuxt/image": "^2.0.0",
    "@nuxtjs/sitemap": "^7.5.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^25.0.3",
    "del-cli": "^7.0.0",
    "eslint": "^9.39.2",
    "storyblok": "^4.12.0",
    "storyblok-markdown-richtext": "^1.0.3",
    "tailwind-merge": "^3.4.0",
    "typescript": "^5.9.3",
    "vite-plugin-devtools-json": "^1.0.0",
    "vite-svg-loader": "^5.1.0"
  }
}
